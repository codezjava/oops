package arraytechniques;

 class ArrayTech {
	 
	  static int intCount(int[] array) {
			int arrayCount = 0;

			for (int c : array) {
				arrayCount = +arrayCount;
				arrayCount++;
			}
       
			return arrayCount;
		
	  static int arraySum(int[] array) {
		  int sum=0;
			int arr[]=new int[] {1,2,3,4,5};
			for(int i=0;i<arr.length;i++) {
				sum+=arr[i];
				return sum;
			}
	

 class ArrayTechniques
{
  public static void main(String[] args) {
	  int arr[]= {1,2,3,4,5,6,7,8,9};
	  
	 
	  System.out.println(ArrayTech.intCount(arr));
	  
	  System.out.println(ArrayTech.arraySum(arr));
	  
	}
			
}


	
 